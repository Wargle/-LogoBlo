<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Text?>

<GridPane minHeight="400.0" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.DecodeWindowController">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints maxHeight="203.0" minHeight="10.0" prefHeight="63.0" vgrow="SOMETIMES" />
    <RowConstraints maxHeight="337.0" minHeight="10.0" prefHeight="337.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
        <TextField fx:id="path" maxWidth="333" GridPane.halignment="LEFT" GridPane.rowIndex="0" GridPane.valignment="CENTER">
            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin></TextField>
      <Button fx:id="parcour" mnemonicParsing="false" text="Parcourir" GridPane.halignment="RIGHT" GridPane.rowIndex="0" />
      <Button fx:id="enter" mnemonicParsing="false" text="&gt;" GridPane.halignment="RIGHT" GridPane.rowIndex="0">
         <GridPane.margin>
            <Insets right="85.0" />
         </GridPane.margin>
      </Button>
      <Text fx:id="mess" strokeType="OUTSIDE" strokeWidth="0.0" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS" />
   </children>
</GridPane>
