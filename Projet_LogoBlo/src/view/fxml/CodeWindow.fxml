            <GridPane.margin>
                <Insets left="5.0" />
            </GridPane.margin></TextField>
      <Button fx:id="parcour" mnemonicParsing="false" text="Parcourir" GridPane.halignment="RIGHT" GridPane.rowIndex="2" />
      <Button fx:id="enter" mnemonicParsing="false" text="&gt;" GridPane.halignment="RIGHT" GridPane.rowIndex="2">
