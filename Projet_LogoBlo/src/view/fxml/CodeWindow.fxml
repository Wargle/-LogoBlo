<?import view.MyTextField?>

<GridPane fx:id="window" minHeight="400.0" minWidth="500.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.CodeWindowController">
    <columnConstraints>
      <ColumnConstraints />
    </columnConstraints>
    <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />      
    </rowConstraints>
    <children>
       <Pane fx:id="code" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS" />
        <MyTextField fx:id="mess" maxWidth="444" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
